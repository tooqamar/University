

@model List<Lib.DAL.vListStudent>
@{
    
    Layout = null;
}
<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">

    <link href="~/Assets/printCss/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Assets/printCss/bootstrap.min.js"></script>
    <script src="~/Assets/printCss/jquery.min.js"></script>
    <title>Report Of Student</title>


</head>
<body style="margin-top:50px;">

    <div class="container">
        





        <div class="row " style="font-weight:bold;">


            <div class="col invoice-details text-right">
                <div class="">Date:   @DateTime.Parse(DateTime.Now.ToString()).ToShortDateString()</div>




            </div>
        </div>






        <div id="table">

            <div class=" ">

                <table class="table">
                    <tr style="background-color: #D3D3D3; color: black;">

                        <th style="width:10%">#</th>
                        <th style="width:10%">Student</th>
                        <th style="width:10%">Course</th>
                        <th style="width:10%">Registration Number</th>
                        <th style="width:10%">Grade</th>
                        <th style="width:10%">Subject</th>





                    </tr>
                    @{ int i = 0; }



                    @foreach (var item in Model)
                    {
                        { i += 1; }
                        <tr>
                            <td>@i</td>


                            <td>
                                <span>@item.Username</span>

                            </td>

                            <td>
                                <span>@item.CourseName</span>

                            </td>
                            <td>
                                <span>@item.RegistrationNumber</span>

                            </td>
                            <td>
                                <span>@item.Name</span>

                            </td>
                            <td>
                                <span>@item.subjectname</span>

                            </td>

                        </tr>
                    }
                </table>


            </div>

        </div>


    </div>
</body>

<style type="text/css">
    .settingcus {
        text-align: right;
        border-top: 1px solid;
    }
</style>
</html>